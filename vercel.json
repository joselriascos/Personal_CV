{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "CV/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "CV/wsgi.py",
      "headers": {
        "Content-Security-Policy": "frame-ancestors 'self' http://localhost:5173 https://joselriascos-portfolio.netlify.app"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
