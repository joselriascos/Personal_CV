{
    "version": 2,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' http://localhost:5173 https://joselriascos-portfolio.netlify.app/;"
          }
        ]
      }
    ],
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "CV/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "CV/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  